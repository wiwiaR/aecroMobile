<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aecro"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0.5" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="28" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_acro"
        android:label="Muda Tela"
        android:roundIcon="@mipmap/ic_acro_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.mudatela.QuartaTela"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mudatela.TerceiraTela"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mudatela.SegundaTela"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.mudatela.MainActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>